    "grunt": "~0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-jshint": "^0.10.0",
    "istanbul": "^0.3.5",
    "load-grunt-tasks": "^2.0.0",
    "mocha": "~2.1.0",
    "mockery": "^1.4.0",
    "sinon": "^1.12.2"
